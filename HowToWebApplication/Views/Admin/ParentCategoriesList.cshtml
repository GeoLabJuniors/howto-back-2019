@model IEnumerable<HowToWebApplication.Models.parentCategories>

@{
    ViewBag.Title = "ParentCategoriesList";
}

<h4> Parent Categories List</h4>

<p>
    @Html.ActionLink("Create New", "CreateParentCategories")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditParentCategories", new { id=item.Id }) |
            @Html.ActionLink("Details", "ParentCategoriesDetails", new { id=item.Id }) |
            @Html.ActionLink("Delete", "DeleteParentCategories", new { id=item.Id })
        </td>
    </tr>
}

</table>
